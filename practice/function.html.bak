<html>
<body>

<h3>JavaScript Functions</h3>

<p id=1></p>
<p id=2></p>
<p id=3></p>
<p id=4></p>
<p id=5></p>
<p id=6></p>

<script>
function add(a,b){
	return a+b;
}

function Celsius(Fahrenheit){
	return (5/9)*(Fahrenheit-32);
}

document.getElementById(1).innerHTML = add(2,4);
document.getElementById(2).innerHTML = Celsius(92);
document.getElementById(3).innerHTML = Celsius;  //returns function defination
document.getElementById(4).innerHTML = "The temperature is "+Celsius(68)+"degree Celsius";

details();
function details(){
	let x=78;
	name="Radhe";
	age=26;
	document.getElementById(5).innerHTML = 
	"Name: "+name+" ("+typeof name+")<br>Age: "+age+" ("+typeof age+")";
}

document.getElementById(6).innerHTML = typeof x +" "+x;
</script>

</body>
</html>